{
    "rules": [
      {
        "description": "At least 8 characters",
        "pattern": ".{8,}"
      },
      {
        "description": "At least one uppercase letter",
        "pattern": "[A-Z]"
      },
      {
        "description": "At least one lowercase letter",
        "pattern": "[a-z]"
      },
      {
        "description": "At least one number",
        "pattern": "[0-9]"
      },
      {
        "description": "At least one special character (!@#$%^&*)",
        "pattern": "[!@#$%^&*]"
      }
    ]
  }
  